<?xml version="1.0" encoding="UTF-8"?>
<extension type="module" version="2.5" client="site" method="upgrade">
	<name>Smugmug Video Viewer</name>
    <element>mod_smugmugvideo</element>
	<creationDate>June 2010</creationDate>
	<author>Cedric Walter</author>
	<authorEmail>cedric.walter@gmail.com</authorEmail>
	<authorUrl>http://www.waltercedric.com</authorUrl>
	<copyright>(C) 2010 Copyright Info</copyright>
	<license>http://www.gnu.org/licenses/gpl-3.0.html GNU/GPL</license>
	<version>2.5.1</version>
	<description>MOD_SMUGMUG_VIDEO_XML_DESCRIPTION</description>
	<scriptfile>twitterbadge.scriptfile.php</scriptfile>

	<install>
	</install>
	<uninstall>
	</uninstall>
	<update>
	</update>
	<files>
		<filename module="mod_smugmugvideo">mod_smugmugvideo.php</filename>
	</files>

	<languages>
		<language tag="en-GB">en-GB.mod_smugmugvideo.ini</language>
	</languages>


	<updateservers>
		<server type="extension" priority="1" name="Extension Update Site">http://updates.waltercedric.com/modules/mod_smugmugvideo/extension.xml
		</server>
		<server type="collection" priority="2" name="Collection Update Site">http://updates.waltercedric.com/update.xml
		</server>
	</updateservers>

	<config>
		<fields name="params">
			<fieldset name="basic">
				<field name="Height" type="text" length="4" default="450"
					label="Height" description="Height of random picture."></field>
				<field name="Width" type="text" length="4" default="450"
					label="Width" description="Width of random picture."></field>
				<field name="Video" type="text"
					default="ZT0xJmk9Mjg1MDE3NTgzJms9cnRxcWomYT0yNTA0NTU5X2YzdGE5JnU9Y21hYw=="
					label="Video" description="Video ID."  size="80"></field>
			</fieldset>
		</fields>
	</config>
</extension>

